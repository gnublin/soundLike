<%= @installed %>

<!DOCTYPE html>
<html>
  <head>
    <title>SoundLike</title>
</head>
    <body>
    <div id="main-login">
      <div id="login">
        <img id="logo-login" src='/logo2.png'/>
          <%= form_for(:session, url: '/new') do |f| %>
            <%= f.label :username %>
            <%= f.text_field :username, class: 'form-control' %>
            <%= f.label :password %>
            <%= f.password_field :password, class: 'form-control' %>
            <%= f.submit "Log in", class: "btn btn-primary" %>
          <% end %>
      </div>
    </div>
  </body>
</html>
